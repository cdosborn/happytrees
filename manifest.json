{
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_title": "Make this page red"
    },
    "chrome_url_overrides": {
        "newtab": "forest.html"
    },
    "description": "Make the current page red",
    "manifest_version": 2,
    "name": "Page Redder",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "version": "2.0"
}
