{
    "background": {
        "persistent": false,
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_title": "Make this page red"
    },
    "chrome_url_overrides": {
        "newtab": "html/forest.html"
    },
    "description": "Find your inner happy tree",
    "content_scripts": [
        {
            "js": [
                "js/content.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ],
    "manifest_version": 2,
    "name": "HappyTrees",
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "version": "2.0"
}
